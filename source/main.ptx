<book><title> PROTEUS Test Environment </title>

	<!-- <xi:include href="matching-questions/chain-rule-mqs.ptx" /> -->
	<chapter><title> Chain-Rule Matching Questions </title>
	<exercise label="chain-rule-matching-00"><title> What's Missing in the Chain Rule? </title>
		<statement>
			<p>
				Drag the missing parts (on the left) needed to complete the derivatives (on the right).
			</p>
			<tabular>
				<col width="5%"/>
				<col halign="left" width="95%"/>
				<row>
					<cell> <p><m>\succ</m></p> </cell>
					<cell> <em>One derivative may require multiple parts (the order doesn't matter).</em> </cell>
				</row>
				<row>
					<cell> <p><m>\succ</m></p> </cell>
					<cell> <em>Some parts may not be needed and should remain in left panel.</em> </cell>
				</row>
			</tabular>
		</statement>
			
		<feedback>
		</feedback>
		<matches widths="20% 80%">
			<match>
				<premise>	
					<me> 4 </me>
				</premise>
				<response>
					<md> 
						<mrow>
							\Big[\sin\left(x^{ \blank{3}{3} }\right)\amp \Big]' = \blank{4}{5} x^3\cos\left(x^{ \blank{3}{3} }\right)
						</mrow>
					</md>	
				</response>
			</match>
			<match>
				<premise>
					<me> x^2 </me>
				</premise>
				<premise>
					<me> 1 </me>
				</premise>
				<response>
					<me> 
						\left[\left(\blank{4}{5}+x\right)^6\right]' = 6\left(2x+\blank{4}{5}\right)\left(\blank{4}{5}+x\right)^5
					</me>
				</response>
			</match>
			<match>
				<premise> 
					<me> 2x </me>
				</premise>
				<premise>
					<me> 4x </me>
				</premise>
				<premise>
					<me> \ln </me>
				</premise>
				<response>
					<me> 
						\left[\blank{4}{5}\!\!\left(e^{\blank{3}{3}} + \blank{4}{5}\right)\right]' = \frac{2e^{\blank{3}{3}}+4}{e^{\blank{3}{3}} + \blank{4}{5}}
					</me>
				</response>
			</match>
			<match>
				<premise>
					<me> 2 </me>
				</premise>
				<premise>
					<me> 2/x </me>
				</premise>
			</match>
		</matches>
	</exercise>
</chapter>

